{
  "name": "task",
  "version": "1.0.0",
  "description": "task management API test project",
  "scripts": {
    "build": "npm run tsc",
    "test": "TS_TEST=true jest",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start:dev": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start": "ts-node src/index.ts",
    "start:prod": "npx knex migrate:latest && npm run build && cross-env NODE_ENV=production node dist/index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "^5.36.2",
    "@tsed/common": "5.36.2",
    "@tsed/core": "5.36.2",
    "@tsed/di": "5.36.2",
    "@tsed/typeorm": "5.36.2",
    "ajv": "^6.10.2",
    "body-parser": "1.19.0",
    "express": "^4.17.1",
    "express-session": "1.17.0",
    "knex": "^0.20.7",
    "knex-alasql": "0.0.3",
    "pg": "^7.17.0",
    "ts-express-decorators": "^5.36.2"
  },
  "devDependencies": {
    "@tsed/testing": "^5.36.2",
    "@types/chai": "^4.2.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.17.2",
    "@types/http-proxy": "^1.16.2",
    "@types/jest": "^24.0.25",
    "@types/mocha": "^5.2.7",
    "@types/node": "12.12.9",
    "@types/pg": "^7.14.1",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "7.5.0",
    "@types/sinon-chai": "3.2.3",
    "@types/supertest": "2.0.8",
    "alasql": "^0.5.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "6.0.3",
    "jest": "^24.9.0",
    "nodemon": "1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "8.5.2",
    "tslint": "5.20.1",
    "typescript": "3.5.3"
  }
}
